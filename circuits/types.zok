const u32 DEPTH = 256;

struct Point {
	field x;
	field y;
}

/// R: Curve point
/// S: Field element. Signature to be verified.
/// A: Curve point. Public part of the key used to create S.
/// M: Message hashed with Poseidon
struct Signature {
	Point r;
	field s;
	Point a;
	field m;
}

struct Tx {
	Point sender;
	Point to;
	field nonce;
	field value;
	Signature sig;
}

struct Account {
	field id;
	field balance;
	field nonce;
}

